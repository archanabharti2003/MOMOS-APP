    <!-- Menue section -->
    <section class="menu container mx-auto py-8">
        <h1 class="text-xl font-bold mb-4">All Momos</h1>
        <div class="grid grid-cols-4 gap-4 row-gap-16">

            <% momos.forEach(function(momos){ %>
                <div class="w-64 bg-green-100 hover:bg-green-50">
                    <img class="h-40 mb-4 mx-auto w-full object-cover" src="/img/<%= momos.image %>" alt="">
                    <div class="text-center">
                        <h2 class="mb-4 text-lg"><%=momos.name%></h2>
                        <span class="size py-1 px-4 rounded-full tect-xs">Pieces:-<%= momos.pieces%></span>
                        <div class="flex items-center justify-around mt-6">
                            <span class="font-bold text-lg">Rs<%=momos.price%></span>
                            <button  data-momos="<%= JSON.stringify(momos) %>" class="add-to-cart py-1 px-6 rounded-full flex items-center font-bold ">
                                <span>+</span>
                                <span class="ml-4">Add</span>
                            </button>
                        </div>
                    </div>
                </div>
           <% }) %>

        </div>

    </section>


    <!--about-->
    <section class="bg-gray-100 py-8">
        <div class="max-w-4xl mx-auto px-4 mb-8">
          <h2 class="text-3xl font-semibold text-gray-800 mb-6">About Us</h2>
          <div class="grid grid-cols-1 md:grid-cols-2 gap-8">
            <div>
              <h3 class="text-xl font-semibold text-gray-700 mb-2">Our Story</h3>
              <p class="text-gray-600">Lorem ipsum dolor sit amet, consectetur adipiscing elit. Nullam lacinia arcu eu velit luctus vestibulum. Mauris pharetra dolor nec diam consequat, in vestibulum tortor vehicula.</p>
            </div>
            <div>
              <h3 class="text-xl font-semibold text-gray-700 mb-2">Our Mission</h3>
              <p class="text-gray-600">Pellentesque habitant morbi tristique senectus et netus et malesuada fames ac turpis egestas. Mauris id dignissim nunc. Aliquam id diam ut eros tristique varius.</p>
            </div>
          </div>
        </div>
      </section>

      <!-- Footer -->
      <section class="footer1">
        <footer class=" text-white py-8 ">
            <div class="max-w-6xl mx-auto px-4">
              <div class="grid grid-cols-1 md:grid-cols-3 gap-4 ">
                <div>
                  <h3 class="text-lg font-semibold mb-4">Contact Us</h3>
                  <p>123 Street Name</p>
                  <p>City, Country</p>
                  <p>Email: info@example.com</p>
                  <p>Phone: +1234567890</p>
                </div>
                <div>
                  <h3 class="text-lg font-semibold mb-4">Follow Us</h3>
                  <div class="flex space-x-4">
                    <a href="#" class="text-white "><i class="fab fa-facebook-f"></i></a>
                    <a href="#" class="text-white "><i class="fab fa-twitter"></i></a>
                    <a href="#" class="text-white "><i class="fab fa-instagram"></i></a>
                  </div>
                </div>
              </div>
              <hr class="my-6 border-t border-gray-700">
              <p class="text-sm text-center">&copy; 2024 Momos Food Delivery App. All rights reserved.</p>
            </div>
          </footer>

      </section>

     
      
      







